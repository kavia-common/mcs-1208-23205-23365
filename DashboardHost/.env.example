# Required environment variables for DashboardHost

# Base URLs of the remote micro frontends (without trailing slash)
# Example: https://asset.example.com
REACT_APP_ASSET_REMOTE_URL=
REACT_APP_TEMPLATES_REMOTE_URL=
REACT_APP_EXPLORER_REMOTE_URL=

# Optional: Site URL for auth/email redirect (if used by a provider like Supabase)
REACT_APP_SITE_URL=
